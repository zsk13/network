/* pages/courseList/courseList.wxss */
.contianer{
  margin-top: 10rpx;
  padding-top: 10rpx;
}
.passwd{
  margin-top: 80rpx;
  margin-bottom: 100rpx;
  margin-left: 100rpx;
  border: solid 1rpx #E5E5E5;
  border-radius:20rpx;
  outline: 0;
  width: 400rpx;
  height:100rpx;
  background: #FFFFFF;
  box-shadow: rgba(0, 0, 0, 0.1) 0rpx 0rpx 8rpx;
}
.course-cell{
  margin-top: 20rpx;
  padding: 20rpx;
  width: 650rpx;
  border-radius: 10rpx;
  border: 1rpx solid gray;
  border-left: 10rpx solid #6F63CC;
  box-shadow: 5rpx 5rpx 5rpx #888888;
}
.select{
  border-bottom: 2rpx solid gray;
  font-size: 24rpx;
  float: right;
}
.commodity_screen {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.2;
  overflow: hidden;
  z-index: 9000;
  color: #fff;
}
.commodity_attr_box {
  z-index: 9999;
  width: 600rpx;
  overflow: hidden;
  position: fixed;
  background: whitesmoke;
  border-radius: 36rpx;
  background: #FFFFFF;
  border: 5rpx solid gray;
}
